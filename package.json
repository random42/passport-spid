{
  "name": "passport-spid",
  "version": "3.0.1",
  "description": "Passport strategy for SPID authentication",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/random42/passport-spid.git"
  },
  "author": {
    "name": "Roberto Sero",
    "email": "roberto.sero42@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "spid",
    "passport",
    "saml",
    "saml2",
    "authentication",
    "italia",
    "italy",
    "digital",
    "identity"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsx --inspect=0.0.0.0:9229 -- test/main.ts",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "bash ./test.sh",
    "check": "biome check",
    "check:staged": "biome check --staged",
    "fix": "biome check --write"
  },
  "dependencies": {
    "@node-saml/node-saml": "^5.1.0",
    "@node-saml/passport-saml": "^5.1.0",
    "@xmldom/xmldom": "^0.8.11",
    "dayjs": "^1.11.18",
    "fast-xml-parser": "^5.3.0",
    "lodash.difference": "^4.5.0",
    "tsx": "^4.20.6",
    "xml-crypto": "^6.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@tsconfig/recommended": "^1.0.10",
    "@types/express": "^4.17.23",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.7.2",
    "@types/xml-crypto": "^1.4.6",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "fs-extra": "^11.3.2",
    "husky": "^9.1.7",
    "ioredis": "^5.8.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3"
  }
}
