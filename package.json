{
  "name": "passport-spid",
  "version": "2.0.5",
  "description": "Passport strategy for SPID authentication",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/random42/passport-spid.git"
  },
  "author": {
    "name": "Roberto Sero",
    "email": "roberto.sero42@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "spid",
    "passport",
    "saml",
    "saml2",
    "authentication",
    "italia",
    "italy",
    "digital",
    "identity"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "ts-node-dev --respawn --inspect=0.0.0.0:9229 -- test/main.ts",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prepare": "run-s build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "bash ./test.sh",
    "check": "biome check",
    "fix": "biome check --write"
  },
  "dependencies": {
    "@node-saml/node-saml": "^4.0.0",
    "@node-saml/passport-saml": "^4.0.1",
    "@xmldom/xmldom": "^0.8.3",
    "dayjs": "^1.11.5",
    "fast-xml-parser": "^4.0.8",
    "lodash.difference": "^4.5.0",
    "xml-crypto": "^2.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@tsconfig/recommended": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.0.0",
    "@types/xml-crypto": "^1.4.2",
    "axios": "^1.1.3",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "fs-extra": "^10.1.0",
    "ioredis": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8"
  }
}
